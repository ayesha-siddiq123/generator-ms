{
    "ingestion_type": "dataset",
    "dataset_name": <DatasetName>,
    "input": {
        "type": "object",
        "properties": {
            "dataset_name": {
                "type": "string"
            },
            "dimensions": {
                "type": "object",
                "properties": {
                    "table": {
                        "type": "string",
                        "pattern": <DimensionTable>
                    },
                    "column": <DimensionCol>,
                    "merge_on_col": {
                        "type": "string",
                        "pattern": <MergeOnCol>
                    }
                },
                "required": [
                    "table",
                    "column",
                    "merge_on_col"
                ]
            },
            "dataset": {
                "type": "object",
                "properties": {
                    "items": {
                        "type": "array",
                        "shouldnotnull": true,
                        "items": {
                            "type": "object",
                             "properties": <DatasetObject>,
                             "required":  <DatasetList>
                        }
                    },
                    "group_by": <GroupByCol>,
                    "aggregate": {
                        "type": "object",
                        "properties": {
                            "function": <AggFunction>,
                            "target_table": {
                                "type": "string",
                                "pattern": <TargetTable>
                            },
                            "numerator_col": {
                                "type": "string",
                                "pattern": <NumeratorCol>
                            },
                            "denominator_col": {
                                "type": "string",
                                "pattern": <DenominatorCol>
                            },
                            "update_cols": <UpdateCol>,
                            "columns": {
                                "type": "array",
                                "shouldnotnull": true,
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "table": {
                                            "type": "string",
                                            "pattern": <AggColTable>
                                        },
                                        "column": <AggCol>
                                    },
                                    "required": [
                                        "column"
                                    ]
                                }
                            },
                            "filters": {
                                "type": "object",
                                "properties": {
                                    "filter_col":{
                                    "type": "string",
                                     "pattern":<FilterCol>
                                     },
                                    "filter_type":{
                                    "type": "string",
                                     "pattern": <FilterType>
                                     },
                                    "filter": {
                                    "type": "string",
                                     "pattern":<Filter>
                                     }
                                },
                                "required": ["filter_col","filter_type","filter"]
                            }
                        },
                        "required": [
                             "target_table"
                        ]
                    }
                },
                "required": [
                    "items"
                ]
            }
        },
        "required": [
            "dataset_name",
            "dataset"
        ]
    }
}